{
	"name": "bun-lockb",
	"displayName": "bun.lockb",
	"publisher": "jaaxxx",
	"description": "View Bun's bun.lockb files",
	"repository": {
		"url": "https://github.com/JacksonKearl/bun-lockb"
	},
	"license": "MIT",
	"version": "0.0.6",
	"icon": "bun-lock.png",
	"engines": {
		"vscode": "^1.75.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": false,
			"description": "bun.lockb files are directly executed, a maliciously crafted file could do the nastiest of things"
		}
	},
	"contributes": {
		"languages": [
			{
				"id": "bun.lockb",
				"aliases": [
					"Bun Lock"
				],
				"extensions": [
					".lockb",
					".lockb.lock"
				],
				"icon": {
					"dark": "./bun-icon-mini.png",
					"light": "./bun-icon-mini.png"
				}
			}
		],
		"customEditors": [
			{
				"viewType": "bun.lockb",
				"displayName": "bun.lockb viewer",
				"selector": [
					{
						"filenamePattern": "*.lockb"
					}
				],
				"priority": "default"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "16.x",
		"@types/vscode": "^1.75.0",
		"typescript": "^4.9.4"
	}
}
